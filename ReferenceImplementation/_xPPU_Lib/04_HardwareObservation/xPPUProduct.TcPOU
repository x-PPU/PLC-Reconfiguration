    <Method Name="Copy" Id="{a392aebb-16ad-4571-b3a2-46e0c4375f0a}">
      <Declaration><![CDATA[METHOD  Copy : BOOL
VAR_INPUT
	i_ptrOther: POINTER TO IProduct;
END_VAR
VAR
	m_ptrOther: POINTER TO xPPUProduct;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF __QUERYPOINTER(i_ptrOther^, m_ptrOther) THEN
	m_isPresent := m_ptrOther^.IsPresent;
	m_isLight := m_ptrOther^.IsLight;
	m_isMetal := m_ptrOther^.IsMetal;
END_IF
]]></ST>
      </Implementation>
    </Method>
