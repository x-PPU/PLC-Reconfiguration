    <Property Name="Material" Id="{dd1d66a9-edcd-4ad0-a48e-4d5be516b67d}">
      <Declaration><![CDATA[PROPERTY Material : EProductTypes]]></Declaration>
      <Get Name="Get" Id="{613c400f-e2f1-4452-8c2b-37d9d64fd36c}">
        <Declaration><![CDATA[VAR
END_VAR
]]></Declaration>
        <Implementation>
          <ST><![CDATA[IF m_isPresent AND m_isLight AND NOT m_isMetal THEN
	Material := EProductTypes.white;
ELSIF m_isPresent AND NOT m_isLight AND NOT m_isMetal THEN
	Material := EProductTypes.black;
ELSIF m_isPresent AND m_isLight AND m_isMetal THEN
	Material := EProductTypes.metal;
ELSIF m_isPresent THEN
	Material := EProductTypes.unknown;
ELSE
	Material := EProductTypes.none;
END_IF
]]></ST>
        </Implementation>
      </Get>
    </Property>
